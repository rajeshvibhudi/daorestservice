<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.daorest.beans.Location" table="LOCATION" >
		<id name="locId" column="LOC_ID">
		 <generator class="native"></generator>
		</id>
		<property name="street" column="STREET_NAME"></property>
		<property name="city" column="CITY_NAME"></property>
		<property name="state" column="STATE_NAME"></property>
		<property name="zipcode" column="ZIPCODE"></property>
	</class>
</hibernate-mapping>